{
    "name": "cristyanhenrich/livemodal",
    "description": "Pacote de modal Livewire para Laravel",
    "type": "library",
    "require": {
        "php": "^8.0.2|^8.1",
        "illuminate/support": "^9.0|^10.10",
        "livewire/livewire": "^2.6|^3.0"
    },
    "autoload": {
        "psr-4": {
            "cristyanhenrich\\livemodal\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "cristyanhenrich\\livemodal\\ModalServiceProvider"
            ]
        }
    }
}
